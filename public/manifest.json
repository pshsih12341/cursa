{
	"manifest_version": 3,
	"name": "My React Extension",
	"version": "1.0",
	"action": {
		"default_popup": "index.html",
		"default_icon": "icon.png"
	},
	"permissions": ["storage"],
	"icons": {
		"128": "icon.png"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	}
}
